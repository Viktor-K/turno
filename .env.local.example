# Local development database connection
# Copy this file to .env.local and adjust values if needed

# PostgreSQL connection for local Docker
POSTGRES_URL="postgres://turno_user:turno_password@localhost:5432/turno_db"
POSTGRES_PRISMA_URL="postgres://turno_user:turno_password@localhost:5432/turno_db?pgbouncer=true"
POSTGRES_URL_NON_POOLING="postgres://turno_user:turno_password@localhost:5432/turno_db"
POSTGRES_USER="turno_user"
POSTGRES_HOST="localhost"
POSTGRES_PASSWORD="turno_password"
POSTGRES_DATABASE="turno_db"

# Keep existing auth variables
VITE_ADMIN_USERNAME="admin"
VITE_ADMIN_PASSWORD_HASH="your_sha256_hash_here"
